{
  "manifest_version": 3,
  "name": "tabber",
  "version": "1.0",
  "description": "Tabber : A Refreshed Newtab Page",
  "author": "Tirtharaj Sinha",
  "chrome_url_overrides": {
    "newtab": "index.html"
  },
  "content_security_policy": "script-src 'self'; object-src 'self';",
  "host_permissions": ["*://*/*"],
  "permissions": ["storage"],
  "icons": {
    "16": "favicon.png",
    "128": "favicon.png"
  }
}
